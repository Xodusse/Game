            <string>///Gun controller
// © 2017 - Jon Harvey

Rate += DELTA;
Reload += DELTA;

if mouse_check_button(mb_left)*GunAuto || mouse_check_button_pressed(mb_left)*!GunAuto
{
    if (Rate&gt;1/GunRate &amp;&amp; Reload&gt;GunReload)
    {
        Rate = 0;
        var Bullet = instance_create(x,y,obj_bullet);
        Bullet.direction = GunDirection;
        Bullet.image_angle = GunDirection;
        Clip--;
        if (Clip&lt;=0)
        {
            Clip = GunClip;
            Reload = 0;
        }
    }
}
</string>
