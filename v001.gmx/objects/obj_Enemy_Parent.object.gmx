            if !(collision_point(x+lengthdir_x(24,image_angle-90),y+lengthdir_y(24,image_angle-90),obj_Enemy_Parent,false,true)){
        
                speed = 3 * FRAME_DELTA;
                
                var PDist = point_distance(x,y,Player.x,Player.y);
                
                if (PDist &lt;= 256){
                    IsTargeting = true;
                    
                    direction = PDir;
                    
                }
                
                else {
                    IsTargeting = false;
                    
                    DirectionTimer -= (1/60) * FRAME_DELTA;
                    
                    if (DirectionTimer &lt;= 0){
                        Direction = irandom_range(0,360);
                        direction = Direction;
                        
                        image_angle = direction;
                        
                        DirectionTimer = random_range(1,5);
                    }             
                }
            }
        }
    }
    
    
    else {
    
        speed = 0;
        x = xprevious;
        y = yprevious;
    }
}

