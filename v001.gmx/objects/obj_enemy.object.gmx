            <string>///Move enemy
// © 2017 - Jon Harvey

WalkDirection = point_direction(x,y,obj_player.x,obj_player.y);
var SumX,SumY;
SumX = 0;
SumY = 0;
for(var i = instance_number(obj_enemy);i&gt;0;i--){

    var Inst = instance_find(obj_enemy,i);
    
    if Inst!=id{
    
        var Distance = distance_to_point(Inst.x,Inst.y);
        if (Distance&lt;Radius){
        
            var NormX = -(Inst.x-x)/Distance;
            var NormY = -(Inst.y-y)/Distance;
            SumX += NormX*(Radius-Distance);
            SumY += NormY*(Radius-Distance);
        }
    }
}
x += SumX+dcos(WalkDirection)*FRAME_DELTA;
y += SumY-dsin(WalkDirection)*FRAME_DELTA;
</string>
