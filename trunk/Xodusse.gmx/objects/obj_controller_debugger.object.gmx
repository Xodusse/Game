    draw_text(8, 85,
        "X = " + string(player_x) + "  Y = " + string(player_y) + "  R = " + string(view_a));
        
    //Speed and Direction:
    draw_text(8, 110,
        "P_Dir = " + string(player_dir) + " P_Speed = " + string(player_speed));
    
    // -- RIGHT --    
            
    draw_set_halign(fa_right);
    
    //FPS:
    draw_text(view_wview[0] - 8, 10,
