            <string>///Update keypresses
// © 2017 - Jon Harvey

//Set key to be assigned whenever a key is released.
if keyboard_check_released(vk_anykey)
{
    Kmap = keyboard_lastkey;
}

//Change key
if Kassign &amp;&amp; keyboard_key
{
    switch(Kmap)
    {
        case KIleft:
        KIleft = keyboard_key;
        break;
        case KIright:
