            <string>//© 2017 - Reece Warren
//GUI Buttons:

draw_self();

//Set draw settings:
draw_set_font(fnt_Exudation_Large);
draw_set_colour($FFFFFF);
draw_set_halign(fa_center);
draw_set_valign(fa_middle);

//Draw button text:

//VSync:
