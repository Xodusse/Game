        tail4 = instance_create(x,y,obj_alien_2_strong_t4);
        tail4.Parent = id;
        tail4.ClampAngle = ClampAngle - (ClampAngle/40);
        tail4.Direction = Direction;            
    }
    
    Create = false;
}

//Movement:
if instance_exists(Parent){

    ClampAngle = Parent.ClampAngle - (Parent.ClampAngle/40);
    var point_dir = point_direction(Parent.x,Parent.y,x,y);
    var angl_diff = clamp(angle_difference(point_dir,Parent.Direction),-ClampAngle,ClampAngle);
    angl_diff = angle_difference(point_dir,Parent.Direction);
    
    Direction = Parent.Direction + angl_diff;
    
    x = Parent.x + 14*(dcos(Direction));
    y = Parent.y + 14*-(dsin(Direction));
    
    image_angle = Direction - 90;
}

//Destruction:
else {
    instance_destroy();
}
