    if (CreationAmount &gt; 0){    
        tail3 = instance_create(x,y,obj_alien_2_strong_t3);
        tail3.Parent = id;
        tail3.ClampAngle = ClampAngle - (ClampAngle/40);
        tail3.Direction = Direction;
