if instance_exists(obj_player){
    
    scr_lighting_draw_level(bac_level,bac_sheet,bac_sheet_n,bac_sheet_she,obj_player.x,obj_player.y);
    
    if (intensity &lt;= 10) &amp;&amp; (toggle = true){
